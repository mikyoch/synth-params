generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model params {
  id        Int      @id @default(autoincrement())
  uid       Int
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())
  dir       Float
  gap       Float
}
